<%include header.ejs%>
<%var i=1%>
<table class="table table-hover" id="para1">
    <thead>
    <tr>
        <th>Rendelés időpontja</th>
        <th>Rendelt pizzák</th>
        <th>Státusz</th>
    </tr>
    </thead>
    <tbody>

    <%users.users[i].rendelesek.forEach(function(item){%>
        <tr>
            <td><%=item.idopont%></td>
            <td>
                <%item.pizzak.forEach(function(item2){%>
                <%=item2.darab%>*<%=item2.pizza%> <br>
                <%}); %>
            </td>
            <td><%=item.statusz%></td>
        </tr>
    <%}); %>
    </tbody>
</table>
<%include footer.ejs%>